                    },
                    "domainName": {
                        "type": "string"
                    }
                },
                "required": [
                    "emailIds"
                ],
                "examples": [
                    {
                        "emailIds": [
                            "<email-id1>",
                            "<email-id2>"
                        ],
                        "role": "USER"
                    }
                ]
            },
